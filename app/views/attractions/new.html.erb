<%= form_for @attraction do |f| -%>
  <%= f.text_field :name %>
  <%= f.number_field :nausea_rating %>
    <%= f.number_field :happiness_rating %>
      <%= f.number_field :tickets %>
        <%= f.number_field :min_height %>
       <%= f.submit "New Attraction", :disable_with => 'Newing...' %>
<% end %>


<% if current_user.admin == true %>
  <%= link_to "New Attraction", new_attraction_path %>
<% end %>